#forceOrigin
function forceOrigin(){
    dest=$PWD".git";
    f="${dest##*/}";
    git ls-remote --get-url ssh://git@72.64.186.174/Users/git/${f} #&>-
    if [ "$?" -ne 0 ]; #fail test
        then
        echo failed to read remote
        makeRemote;forceOrigin;
        else
            echo suceeded to read
            git remote add origin ssh://git@72.64.186.174/Users/git/${f};
            git remote set-url origin ssh://git@72.64.186.174/Users/git/${f};
            git remote -v
        fi
}
